{
  "name": "apple-music",
  "version": "1.0.11",
  "description": "A node-pre-gyp module which provides the ability to publish to GitHub releases.",
  "bin": "./bin/node-pre-gyp-github.js",
  "main": "index.js",
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild --release",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn publish && git push --follow-tags; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willemdjong/apple-music.git"
  },
  "keywords": [
    "node-pre-gyp",
    "binary",
    "bindings",
    "c++",
    "c",
    "native",
    "addon",
    "module",
    "github",
    "releases"
  ],
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "bindings": "^1.5.0",
    "commander": "7.2.0",
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.0.2",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "sinon": "13.0.1",
    "prebuild": "^12.0.0"
  },
  "author": "Bill Christo",
  "license": "MIT",
  "binary": {
    "host": "https://github.com/willemdjong/apple-music/releases/download/",
    "remote_path": "v{version}",
    "module_name": "apple-music",
    "module_path": "./build/Release/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  }
}